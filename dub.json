	"description": "Moss Source + Binary Formats",
	"license": "ZLib",
	"version": "0.0.1",
	"name": "moss-format",
	"targetType" : "none",
	"dependencies": {
		"moss-format:source": "*",
		"moss-format:binary": "*",
	},
	"subPackages": [
		{
			"name": "source",
			"targetType": "library",
			"sourcePaths": ["source/moss/format/source/"],
			"dependencies": {
				"moss-core": ">=0.0.1",
				"dyaml": {
					"path": "external/dyaml",
				},
				"tinyendian": {
					"path": "external/tinyendian",
				}
			}
		},
		{
			"name": "binary",
			"targetType": "library",
			"sourcePaths": ["source/moss/format/binary/"],
			"dependencies": {
				"moss-core": ">=0.0.1",
				"zstd": {
					"path": "external/zstd",
				}
			}
		}
	]
}
